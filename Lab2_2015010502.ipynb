    "import random\n",
    "import time\n",
    "import numpy as np\n",
    "\n",
    "inicio = time.time()\n",
    "randnums= np.random.normal(500,30,10000000) \n",
    "suma = 0 \n",
    "\n",
    "for i in randnums:\n",
    "    if i <= 500000:\n",
    "        suma+=i\n",
    "\n",
    "print (suma)\n",
    "print(randnums)\n",
    "print('Duracion: {} segundos'.format(time.time() - inicio))"
   ]
